Grammar

    0 $accept: program "EOF"

    1 program: body "end" subprograms

    2 body: declarations statements

    3 declarations: declarations type vars
    4             | declarations "record" fields "endrec" vars
    5             | declarations "data" vals
    6             | %empty

    7 type: "integer"
    8     | "real"
    9     | "logical"
   10     | "character"

   11 vars: vars "," undef_variable
   12     | vars "," error
   13     | undef_variable

   14 undef_variable: "id" "(" dims ")"
   15               | "id"

   16 dims: dims "," dim
   17     | dim

   18 dim: "iconst"
   19    | "id"

   20 fields: fields field
   21       | field

   22 field: type vars
   23      | "record" fields "endrec" vars

   24 vals: vals "," "id" value_list
   25     | "id" value_list

   26 value_list: "/" values "/"

   27 values: values "," value
   28       | value

   29 value: repeat "*" "+|-" constant
   30      | repeat "*" constant
   31      | repeat "*" "string"
   32      | "+|-" constant
   33      | constant
   34      | "string"

   35 repeat: "iconst"
   36       | %empty

   37 constant: "iconst"
   38         | "rconst"
   39         | "lconst"
   40         | "cconst"

   41 statements: statements labeled_statement
   42           | labeled_statement

   43 labeled_statement: label statement
   44                  | statement

   45 label: "iconst"

   46 statement: simple_statement
   47          | compound_statement

   48 simple_statement: assignment
   49                 | goto_statement
   50                 | if_statement
   51                 | subroutine_call
   52                 | io_statement
   53                 | "continue"
   54                 | "return"
   55                 | "stop"

   56 assignment: variable "=" expression
   57           | variable "=" "string"

   58 variable: variable ":" "id"
   59         | variable "(" expressions ")"
   60         | "id"

   61 expressions: expressions "," expression
   62            | expression

   63 expression: expression ".OR." expression
   64           | expression ".AND." expression
   65           | expression ".GT. | .GE. | .LT. | .LE. | .EQ. | .NE." expression
   66           | expression "+|-" expression
   67           | expression "*" expression
   68           | expression "/" expression
   69           | expression "**" expression
   70           | ".NOT" expression
   71           | "+|-" expression
   72           | variable
   73           | constant
   74           | "(" expression ")"

   75 goto_statement: "goto" label
   76               | "goto" "id" "," "(" labels ")"

   77 labels: labels "," label
   78       | label

   79 if_statement: "if" "(" expression ")" label "," label "," label
   80             | "if" "(" expression ")" simple_statement
   81             | error "(" expression ")" label "," label "," label
   82             | error "(" expression ")" simple_statement

   83 subroutine_call: "call" variable

   84 io_statement: "read" read_list
   85             | "write" write_list

   86 read_list: read_list "," read_item
   87          | read_item

   88 read_item: variable
   89          | "(" read_list "," "id" "=" iter_space ")"

   90 iter_space: expression "," expression step

   91 step: "," expression
   92     | %empty

   93 write_list: write_list "," write_item
   94           | write_item

   95 write_item: expression
   96           | "(" write_list "," "id" "=" iter_space ")"
   97           | "string"

   98 compound_statement: branch_statement
   99                   | loop_statement

  100 branch_statement: "if" "(" expression "then" body tail

  101 tail: "else" body "endif"
  102     | "endif"

  103 loop_statement: "do" "id" "=" iter_space body "enddo"

  104 subprograms: subprograms subprogram
  105            | %empty

  106 subprogram: header body "end"

  107 header: type "function" "id" "(" formal_parameters ")"
  108       | "subroutine" "id" "(" formal_parameters ")"
  109       | "subroutine" "id"

  110 formal_parameters: type vars "," formal_parameters
  111                  | type vars


